{
	"name": "imarc/opus",
	"description": "Multi-framework asset and module packaging for composer",
	"keywords": ["package management"],
	"type": "composer-installer",
	"license": "MIT",
	"authors": [
		{
			"name": "Matthew J. Sahagian",
			"email": "matt@imarc.net",
			"role": "Developer"
		}
	],
	"require": {
		"php": ">=5.3.0"
	},
	"require-dev": {
		"composer/composer": "^2.0"
	},
	"autoload": {
		"psr-0": {
			"iMarc\\Opus": "src/",
			"Diff": "src/"
		}
	},
	"extra": {
		"class": "iMarc\\Opus\\Processor"
	}
}
